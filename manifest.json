{
  "manifest_version": 2,

  "name": "Squirrel",
  "description": "This extension will check what tabs you are using for aiding your coding",
  "version": "3.1",
  "minimum_chrome_version": "23",

  "browser_action": {
    "default_icon": "Squirrel.png"
  },

	"background": {
		"scripts": ["getURL.js"],
		"persistent": true
	},
  
  "permissions": [
	"tabs",
	"activeTab",
	"storage",
	 "webRequest",
          "*localhost:8080/"        
	]
}